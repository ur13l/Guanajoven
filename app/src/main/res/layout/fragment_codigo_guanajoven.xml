<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:layout_editor_absoluteY="81dp"
    tools:layout_editor_absoluteX="0dp">

    <LinearLayout
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginBottom="27dp"
        android:layout_marginEnd="20dp"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="27dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1">

        <ImageView
            android:id="@+id/imageView11"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:srcCompat="@drawable/placeholder" />

        <android.support.constraint.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            tools:layout_editor_absoluteY="128dp"
            tools:layout_editor_absoluteX="0dp">

            <ImageView
                android:id="@+id/icono_nombre"
                android:layout_width="32dp"
                android:layout_height="36dp"
                app:srcCompat="@drawable/ic_person_black_24dp"
                android:layout_marginLeft="8dp"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                android:layout_marginTop="8dp" />

            <android.support.design.widget.TextInputLayout
                android:id="@+id/input_nombre"
                android:layout_width="282dp"
                android:layout_height="36dp"
                android:layout_marginEnd="8dp"
                app:layout_constraintLeft_toRightOf="@+id/icono_nombre"
                android:layout_marginLeft="8dp"
                app:layout_constraintTop_toTopOf="parent"
                android:layout_marginTop="8dp"
                android:layout_marginRight="8dp"
                app:layout_constraintRight_toRightOf="parent">

                <EditText
                    android:id="@+id/nombre_usuario"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:enabled="false"
                    android:hint="Nombre"
                    android:inputType="none"
                    android:maxLength="50"
                    android:singleLine="true"
                    android:textSize="16dp"
                    android:typeface="normal" />

            </android.support.design.widget.TextInputLayout>

            <ImageView
                android:id="@+id/icono_email"
                android:layout_width="32dp"
                android:layout_height="36dp"
                app:srcCompat="@drawable/ic_email_black_24dp"
                android:layout_marginTop="8dp"
                app:layout_constraintTop_toBottomOf="@+id/icono_nombre"
                android:layout_marginLeft="8dp"
                app:layout_constraintLeft_toLeftOf="parent" />

            <android.support.design.widget.TextInputLayout
                android:id="@+id/input_email"
                android:layout_width="282dp"
                android:layout_height="36dp"
                android:layout_marginEnd="8dp"
                app:layout_constraintLeft_toRightOf="@+id/icono_email"
                android:layout_marginLeft="8dp"
                android:layout_marginTop="8dp"
                app:layout_constraintTop_toBottomOf="@+id/input_nombre"
                android:layout_marginRight="8dp"
                app:layout_constraintRight_toRightOf="parent">

                <EditText
                    android:id="@+id/email"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:enabled="false"
                    android:hint="Correo"
                    android:inputType="none"
                    android:maxLength="50"
                    android:singleLine="true"
                    android:textSize="16dp"
                    android:typeface="normal" />

            </android.support.design.widget.TextInputLayout>

            <ImageView
                android:id="@+id/icono_genero"
                android:layout_width="32dp"
                android:layout_height="36dp"
                android:layout_weight="1"
                app:srcCompat="@drawable/ic_wc_black_24dp"
                android:layout_marginTop="8dp"
                app:layout_constraintTop_toBottomOf="@+id/icono_email"
                android:layout_marginLeft="8dp"
                app:layout_constraintLeft_toLeftOf="parent" />

            <android.support.design.widget.TextInputLayout
                android:id="@+id/input_genero"
                android:layout_width="76dp"
                android:layout_height="36dp"
                app:layout_constraintLeft_toRightOf="@+id/icono_genero"
                android:layout_marginLeft="8dp"
                app:layout_constraintRight_toLeftOf="@+id/icono_fecha"
                android:layout_marginRight="8dp"
                android:layout_marginTop="8dp"
                app:layout_constraintTop_toBottomOf="@+id/input_email"
                app:layout_constraintHorizontal_bias="0.0"
                tools:layout_editor_absoluteX="49dp">

                <EditText
                    android:id="@+id/genero"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:enabled="false"
                    android:hint="Genero"
                    android:inputType="none"
                    android:maxLength="50"
                    android:singleLine="true"
                    android:textSize="16dp"
                    android:typeface="normal" />

            </android.support.design.widget.TextInputLayout>

            <ImageView
                android:id="@+id/icono_fecha"
                android:layout_width="35dp"
                android:layout_height="37dp"
                app:srcCompat="@drawable/ic_cake_pink_200_24dp"
                tools:layout_editor_absoluteX="138dp"
                android:layout_marginTop="8dp"
                app:layout_constraintTop_toBottomOf="@+id/input_email" />

            <android.support.design.widget.TextInputLayout
                android:id="@+id/input_fecha"
                android:layout_width="149dp"
                android:layout_height="36dp"
                app:layout_constraintLeft_toRightOf="@+id/icono_fecha"
                android:layout_marginLeft="8dp"
                android:layout_marginTop="8dp"
                app:layout_constraintTop_toBottomOf="@+id/input_email">

                <EditText
                    android:id="@+id/fecha"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:enabled="false"
                    android:hint="Fecha de nacimiento"
                    android:inputType="none"
                    android:maxLength="50"
                    android:singleLine="true"
                    android:textSize="16dp"
                    android:typeface="normal" />
            </android.support.design.widget.TextInputLayout>

            <ImageView
                android:id="@+id/icono_curp"
                android:layout_width="32dp"
                android:layout_height="36dp"
                app:srcCompat="@drawable/ic_account_box_black_24dp"
                android:layout_marginTop="8dp"
                app:layout_constraintTop_toBottomOf="@+id/icono_genero"
                android:layout_marginLeft="8dp"
                app:layout_constraintLeft_toLeftOf="parent" />

            <android.support.design.widget.TextInputLayout
                android:id="@+id/input_curp"
                android:layout_width="282dp"
                android:layout_height="36dp"
                android:layout_marginEnd="8dp"
                android:layout_marginTop="8dp"
                app:layout_constraintTop_toBottomOf="@+id/icono_fecha"
                app:layout_constraintLeft_toRightOf="@+id/icono_curp"
                android:layout_marginLeft="8dp"
                android:layout_marginRight="8dp"
                app:layout_constraintRight_toRightOf="parent">

                <EditText
                    android:id="@+id/curp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:enabled="false"
                    android:hint="CURP"
                    android:inputType="none"
                    android:maxLength="50"
                    android:singleLine="true"
                    android:textSize="16dp"
                    android:typeface="normal" />

            </android.support.design.widget.TextInputLayout>

            <ImageView
                android:id="@+id/icono_cp"
                android:layout_width="32dp"
                android:layout_height="34dp"
                android:layout_weight="1"
                app:srcCompat="@drawable/ic_confirmation_number_black_24dp"
                android:layout_marginTop="8dp"
                app:layout_constraintTop_toBottomOf="@+id/icono_curp"
                android:layout_marginLeft="8dp"
                app:layout_constraintLeft_toLeftOf="parent" />

            <android.support.design.widget.TextInputLayout
                android:id="@+id/input_cp"
                android:layout_width="76dp"
                android:layout_height="36dp"
                android:layout_marginTop="8dp"
                app:layout_constraintTop_toBottomOf="@+id/input_curp"
                app:layout_constraintLeft_toRightOf="@+id/icono_cp"
                android:layout_marginLeft="8dp"
                tools:layout_editor_absoluteX="48dp">

                <EditText
                    android:id="@+id/cp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:enabled="false"
                    android:hint="CÃ³digo postal"
                    android:inputType="none"
                    android:maxLength="50"
                    android:singleLine="true"
                    android:textSize="16dp"
                    android:typeface="normal" />
            </android.support.design.widget.TextInputLayout>

            <ImageView
                android:id="@+id/icono_estado"
                android:layout_width="35dp"
                android:layout_height="37dp"
                app:srcCompat="@drawable/ic_place_white_24dp"
                android:layout_marginTop="8dp"
                app:layout_constraintTop_toBottomOf="@+id/input_curp"
                app:layout_constraintLeft_toRightOf="@+id/input_cp"
                android:layout_marginLeft="13dp" />

            <android.support.design.widget.TextInputLayout
                android:id="@+id/input_estado"
                android:layout_width="146dp"
                android:layout_height="36dp"
                android:layout_marginTop="8dp"
                app:layout_constraintTop_toBottomOf="@+id/input_curp"
                android:layout_marginRight="16dp"
                app:layout_constraintRight_toRightOf="parent"
                tools:layout_editor_absoluteX="182dp">

                <EditText
                    android:id="@+id/estado"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:enabled="false"
                    android:hint="Estado"
                    android:inputType="none"
                    android:maxLength="50"
                    android:singleLine="true"
                    android:textSize="16dp"
                    android:typeface="normal" />

            </android.support.design.widget.TextInputLayout>

        </android.support.constraint.ConstraintLayout>

    </LinearLayout>
</android.support.constraint.ConstraintLayout>